{
  "name": "Save AB Loop",
  "identifier": "com.example.iina-plugin-template",
  "version": "0.1.0",
  "description": "Save AB loops as individual files",
  "author": {
    "name": "IINA Developers",
    "email": "doesnotexist@doesnotexist.doesnotexist",
    "url": "https://iina.io"
  },
  "entry": "src/index.js",
  "permissions": [
    "show-osd",
    "file-system",
    "network-request"
  ],
  "allowedDomains": ["*"],
  "preferencesPage": "preferences.html",
  "preferenceDefaults": {
    "ffmpeg_path": "/usr/local/bin/ffmpeg",
    "save_dir": null,
    "max_file_name_length": null
  }
}